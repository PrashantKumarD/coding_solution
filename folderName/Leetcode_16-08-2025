class Solution {
    public int maximum69Number (int num) {
        StringBuilder stb = new  StringBuilder(); stb.append(num);
        String str = stb.toString();
        int res=0; boolean f=true;
        for(char ch : str.toCharArray()){
            if(ch=='9') res=res*10+9;
            else if(ch=='6'&&f){
                res=res*10+9;
                f=false;
            }
            else res=res*10+6;
        }
        return res;
    }
}
