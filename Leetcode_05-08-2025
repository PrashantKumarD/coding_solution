class Solution {
    public int numOfUnplacedFruits(int[] fruits, int[] baskets) {
        boolean[] isfilled = new boolean[baskets.length];
        int count=0;
        for(int i : fruits){
            boolean getbasket = false;
            for(int j=0;j<baskets.length;j++){
                if(!isfilled[j]&&baskets[j]>=i){
                    isfilled[j]=true;
                    getbasket = true;
                    break;
                }
            }
            if(!getbasket){
                count++;
            }
        }
        return count;
    }
}
